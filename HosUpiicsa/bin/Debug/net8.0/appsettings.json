{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=HotelReservas;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "OmniSharp": {
    "RoslynExtensionsOptions": {
      "EnableAnalyzersSupport": true,
      "LocationPaths": []
    },
    "FormattingOptions": {
      "EnableEditorConfigSupport": true
    }
  },
  "StaticFiles": {
    "Headers": {
      "Cache-Control": "public,max-age=3600"
    }
  }
}